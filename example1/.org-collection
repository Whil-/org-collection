 :name "example1"
 :customization (;; Main org collection customization
                 (org-use-speed-commands nil)
                 (org-todo-keywords ((sequence "MAYBE" "TODO" "DONE")))
                 (org-log-done nil)

                 ;; Agenda customizations
                 (org-agenda-files ("agenda1.org" "agenda2.org"))
                 (org-agenda-file-regexp ".*\\.org\\'")))
